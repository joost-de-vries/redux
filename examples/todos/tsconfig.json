{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react",
    "sourceMap": true,
    "noImplicitAny":true,
    "noFallthroughCasesInSwitch":true,
    "noEmitOnError": true,
    "noImplicitReturns":true,
    "noImplicitThis":true,
 "strictNullChecks":true,
    "outDir": "./build",
    "lib": ["es6","dom"]
  },
  "exclude": [
    "node_modules",
    "build"
  ]
}
